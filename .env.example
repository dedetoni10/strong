# Strong Warehouse Management System - Environment Configuration

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost/strong_warehouse

# Session Security
SESSION_SECRET=your_very_secure_session_secret_key_here_change_this_in_production

# Application Settings
FLASK_ENV=production
FLASK_DEBUG=False

# Optional: Shopee API (if using Shopee integration)
SHOPEE_API_KEY=your_shopee_api_key_here
SHOPEE_SHOP_ID=your_shop_id_here
SHOPEE_ACCESS_TOKEN=your_access_token_here

# Performance Settings (already optimized)
SQLALCHEMY_POOL_SIZE=100
SQLALCHEMY_MAX_OVERFLOW=200
SQLALCHEMY_POOL_TIMEOUT=5